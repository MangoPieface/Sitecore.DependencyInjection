<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Sitecore.DependencyInjection</id>
    <version>1.0.3.0</version>
    <title>Sitecore Dependency Injection Module</title>
    <authors>Eduserv</authors>
    <owners>Eduserv</owners>
    <licenseUrl>https://github.com/Eduserv/Sitecore.DependencyInjection/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/Eduserv/Sitecore.DependencyInjection</projectUrl>
	<iconUrl>https://raw.githubusercontent.com/Eduserv/Sitecore.DependencyInjection/master/NugetPackage/eduserv.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A Nuget package for Sitecore solutions that allows development teams to easily and safely use their preferred inversion of control (IoC) container with Sitecore. This is useful for dependency injection into Sitecore MVC controllers.</description>
    <dependencies>
      <dependency id="Microsoft.Web.Infrastructure" version="1.0.0.0" />
      <dependency id="WebActivatorEx" version="2.0.6" />
      <dependency id="Castle.Core" version="3.2.0" />
      <dependency id="Castle.Windsor" version="3.2.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="lib\Sitecore.DependencyInjection.dll" target="lib" />
    <file src="content\zzz.Sitecore.DependencyInjection.config" target="content\App_Config\Include\zzz.Sitecore.DependencyInjection.config" />
    <file src="content\WindsorInstallerStarter.cs.pp" target="content\App_Start\WindsorInstallerStarter.cs.pp" />
    <file src="content\MvcControllersInstaller.cs.pp" target="content\App_Start\Installers\MvcControllersInstaller.cs.pp" />
    <file src="content\ServicesInstaller.cs.pp" target="content\App_Start\Installers\ServicesInstaller.cs.pp" />
  </files>
</package>